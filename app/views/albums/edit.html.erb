<section id="main-content">
  <section class="wrapper"></section>
  <div class="panel">
    <header class="panel-heading">
      <h2>
        Edit Album
      </h2>
    </header>
    <div class="panel-body">
      <%= form_for(@album, url: {action: "update"}, method: :put) do |f| %>
        <%= render "form", f: f %>
      <% end %>
    </div>
  </div>
</section>
